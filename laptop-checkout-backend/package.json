{
  "name": "laptop-check-out",
  "version": "1.0.0",
  "description": "An app that tracks laptop usage",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "author": "Alex Jobe",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.3",
    "method-override": "^3.0.0",
    "mongoose": "^5.6.10",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.3.0"
  },
  "devDependencies": {
    "electron": "^6.0.3",
    "electron-builder": "^21.2.0",
    "nodemon": "^1.19.1"
  },
  "build": {
    "appId": "com.mai.laptopcheckout",
    "productName": "Laptop Checkout",
    "target": "NSIS",
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "perMachine": false,
      "oneClick": false
    },
    "files": [
      "build",
      "models",
      "node_modules",
      "routes",
      "utils",
      "package.json",
      "main.js",
      "app.js"
    ]
  }
}
